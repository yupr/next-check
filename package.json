{
  "name": "next-check",
  "private": true,
  "scripts": {
    "dev": "next",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:local": "ts-node --project tsconfig.server.json server/index.ts",
    "build": "next build",
    "analyze": "ANALYZE=true yarn build",
    "export": "next build && next export",
    "serve": "yarn export && serve ./out",
    "lint": "eslint '**/*.{ts,tsx}'",
    "fmt": "prettier --write '**/*.{css,scss,js,jsx,ts,tsx}'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.3",
    "@mui/material": "^5.10.5",
    "@next/bundle-analyzer": "^12.3.0",
    "@tanstack/react-query": "^4.3.4",
    "@tanstack/react-query-devtools": "^4.3.5",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "next": "^12.3.0",
    "next-compose-plugins": "^2.2.1",
    "normalize.css": "^8.0.1",
    "pixi.js": "^6.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.35.0",
    "react-loader-spinner": "^5.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "serve": "^14.0.1",
    "typescript": "^4.8.3"
  }
}
