{
  "name": "next-check",
  "private": true,
  "scripts": {
    "dev": "next",
    "prd:start": "next start",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "dev:local": "ts-node --project server/tsconfig.json server/index.ts",
    "build": "next build",
    "export": "next build && next export",
    "delete:sourcemap": "find out/ -name '*.map' -delete",
    "test": "jest --watch",
    "analyze": "ANALYZE=true yarn build",
    "lint": "eslint '**/*.{ts,tsx}' --cache",
    "fmt": "prettier --write --cache --cache-strategy metadata '**/*.{css,scss,js,jsx,ts,tsx}'",
    "gen:api-cli": "bash src/shell/genOpenapiCli.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.15",
    "@mui/material": "^5.10.15",
    "@next/bundle-analyzer": "^14.0.4",
    "@tanstack/react-query": "^4.35.0",
    "@tanstack/react-query-devtools": "^4.35.0",
    "axios": "^1.1.3",
    "dayjs": "^1.11.7",
    "gsap": "^3.11.3",
    "next": "^13.0.4",
    "next-compose-plugins": "^2.2.1",
    "normalize.css": "^8.0.1",
    "pixi.js": "^7.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.0.3",
    "msw": "^1.3.2",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "resolutions": {
    "enhanced-resolve": "5.10.0"
  }
}
